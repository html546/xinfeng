/* pages/dingzhi/dingzhi.wxss */
page{
  background: #F0EFF5;
}
.container2{
  width: 750rpx;
  height: 100%;
}
.zhuwo{
  padding: 100rpx 0rpx 20rpx 0rpx;
  text-indent: 16rpx;
  margin-bottom: 20rpx;
}
.zhuwo text.huanjing,.youhua text.youhua2{
  font-size: 24rpx;
  position: relative;
  top: -12rpx;
}
.zhuwo .zhuwocon{
  width: 100%;
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  position: relative;
}
.zhuwo .zhuwocon text,.tiaojiecon text{
  color: #000;
  font-weight: 600;
  font-family: '微软雅黑';
  font-size:28rpx;
}
.zhuwo .zhuwocon text text{
  float: right;
  margin-right: 50rpx;
  color: #65CBFA;
}
.zhuwo .zhuwocon image{
  display:inline-block;
  width:34rpx;
  height:34rpx;
  position:absolute;
  right:16rpx;
  top:23rpx;
}
.tiaojiecon{
  width: 100%;
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  text-indent: 16rpx;
  margin-bottom: 20rpx;
}
.tiaojiecon switch,.yejian switch{
  float: right;
  margin-right: 20rpx;
}
.youhua{
  text-indent: 16rpx;
  margin-bottom: 120rpx;
}
.youhuatar{
  width: 100%;
  height: auto;
  background: #fff;
}
.maer{
  width: 100%;
  height: 80rpx;
  position: relative;
}
.maer icon[type=success_no_circle]{
  margin-top: 20rpx;
  margin-right: 20rpx;
}
.maer icon[type=info]{
  position: absolute;
  right: 30rpx;
  top: 20rpx;
}
.maer2 text{
  line-height:80rpx;
  margin-top:20rpx;
}
.yejian{
  width: 100%;
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  text-indent: 16rpx;
  position: relative;
}